{
  "pipeline": {
    "test": {
      "dependsOn": ["build"]
    },
    "clean": {
      "dependsOn": ["^clean"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "build": {
      "dependsOn": [
        "clean",
        "lint",
        "$JITSU_KEY",
        "$JITSU_SERVER",
        "$SEGMENT_KEY",
        "$POSTGREST_URL",
        "$POSTGREST_API_KEY"
      ]
    },
    "prettier:fix": {
      "dependsOn": ["^prettier:fix"]
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": ["tsconfig.json"]
}
